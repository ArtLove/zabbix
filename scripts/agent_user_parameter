UserParameter=mHDD.health,sudo smartctl -H /dev/sda |grep "test"| cut -f2 -d: |tr -d " "

UserParameter=lmsensors[*],/etc/zabbix/scripts/lmsensors.sh "none" $1

UserParameter=sensor.mb,sensors|grep 'MB Temperature'| cut -c23-24

UserParameter=sensor.cpu,sensors|grep 'CPU Temperature'| cut -c23-24

UserParameter=sensor.sda,sudo hddtemp -nq /dev/sda

UserParameter=sensor.sdb,sudo hddtemp -nq /dev/sdb

UserParameter=nginx[*],/etc/zabbix/scripts/nginx-stats.sh "none" "$1" "$2"

UserParameter=iostat[*],/etc/zabbix/scripts/iostat.sh "none" "$1" "$2"

UserParameter=mysql-stats[*],/etc/zabbix/scripts/mysql-stats.sh "none" "$1" DB_USER DB_PASSWORD #!!!CAUTION

UserParameter=apache2[*],/etc/zabbix/scripts/apache-stats.sh "none" "$1" "$2"
